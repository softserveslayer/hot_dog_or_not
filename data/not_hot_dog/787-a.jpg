        function signNewsletter(el) {
            var form = $(el).parent();
            var txtemail = form.find("input").eq(0);
            var txtzip = form.find("input").eq(1);

            var msg = "";
            if (txtemail.val() == "") {
                msg += "Email Address is required.\n";
            }
            else if (!isEmail(txtemail.val())) {
                msg += "Email Address is invalid.\n";
            }
            if (txtzip.val() == "") {
                msg += "Zip is required.";
            }
            if (msg != "") {
                $(el).blur();
                alert(msg);
                setTimeout(function () { $(el).focus(); }, 300);
            }
            else {
                $.post("/AJAX/Newsletter.ashx", { 'email': txtemail.val(), 'zip': txtzip.val() }, function (data) {
