<script>
    ; (function () {

        var galleryRoot = $('.tos-gallery');
        var pswpElement = $('.pswp')[0];

        var options = {
            index: 0
        },
				slidesElements,
				slides;

        function initPhotoSwipeGallery(rootSelector) {
            slidesElements = $(rootSelector).children('figure');
            slidesElements.each(function (index, elem) {
                $(elem).attr('data-index', index);
            })
            slides = buildPhotoSwipeCollection(slidesElements);
        }

        function buildPhotoSwipeCollection(elements) {
            var arr = [];
            $.each(elements, function (index, elem) {
                arr.push({
                    src: $(elem).find('img').attr('src'),
                    w: 600,
                    h: 400
                })
            })
            return arr;
        }

        function onThumbnailClick(event) {
            event.preventDefault();
            console.log(event)
            var target = event.target,
					parent = $(target).closest('figure');

            var options = {
                index: parseInt(parent.attr('data-index'), 10)
            };


            // PhotoSwipeUI_Default is exposed in a JS file included after the main Photoswipe JS lib
            var gallery = new PhotoSwipe(pswpElement, PhotoSwipeUI_Default, slides, options);
            gallery.init();
        }

        galleryRoot.on('click', 'figure > a', onThumbnailClick);
        initPhotoSwipeGallery('.tos-gallery');

    })();

</script>
